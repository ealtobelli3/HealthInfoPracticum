<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practicum">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/healthcare"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/healthcare"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practicum">
        <activity android:name=".AddOfficeScreen" android:theme="@style/Theme"></activity>
        <activity android:name=".GetAllOffices" android:theme="@style/Theme"></activity>
        <activity android:name=".OfficeDetail" android:theme="@style/Theme"></activity>
        <activity
            android:name=".SearchScreen"
            android:theme="@style/Theme" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>